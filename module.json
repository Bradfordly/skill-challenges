{
    "name": "skill-challenges",
    "title": "Skill Challenges for DnD5e",
    "description": "Run 4E-style Skill Challenge encounters in Foundry VTT's dnd5e system.",
    "system": ["dnd5e"],
    "version": "{{version}}",
    "author": "Bradfordly",
    "authors": [
      {
        "name": "Bradfordly",
        "url": "https://github.com/Bradfordly"
      },
    ],
    "socket": true,
    "scripts": [],
    "esmodules": ["/src/skill-challenges.js"],
    "styles": ["/styles/skill-challenges.css"],
    "dependencies": [
      {
        "name": "socketlib", "type": "module"
      },
      {
        "name": "lib-wrapper", "type": "module"
      }
